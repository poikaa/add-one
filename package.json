{
  "name": "@ikim23/add-one",
  "version": "1.1.0",
  "description": "Increment number",
  "homepage": "https://github.com/ikim23/add-one#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ikim23/add-one.git"
  },
  "bugs": {
    "url": "https://github.com/ikim23/add-one/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "author": "ikim23",
  "main": "dist/index.js",
  "module": "dist/add-one.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prepare": "husky install",
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --no-cache",
    "lint": "tsdx lint"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^7.0.2",
    "semantic-release": "^18.0.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "message": "chore(release): ${nextRelease.version}"
        }
      ]
    ]
  }
}
